             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Words_MVVM.Views.ExamplesPage">

    <ListView ItemsSource="{Binding Ex}"
              SelectionMode="None">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Tr[0].Text}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>
